{
  "name": "ngx-cron",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "npm run build:lib && ng build --prod",
    "build:demo": "npm run build:lib && cross-env NODE_ENV=production ng build --prod --base-href=\"/ngx-cron/\"",
    "build:lib": "ng build @swimlane/ngx-cron --prod && npm run copy-files",
    "copy-files": "cp README.md dist/swimlane/ngx-cron/README.md && cp LICENSE dist/swimlane/ngx-cron/LICENSE",
    "package": "npm run build:lib",
    "test:unit": "ng test @swimlane/ngx-cron",
    "test:watch": "ng test @swimlane/ngx-cron --watch",
    "test:ci": "ng test @swimlane/ngx-cron --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test": "run-p lint test:unit",
    "ci": "run-s lint prettier:ci test:ci",
    "lint": "ng lint",
    "predeploy": "npm run build:demo",
    "deploy": "angular-cli-ghpages --dir ./dist/ngx-cron --name=Swimlane --email=developer@swimlane.com",
    "prepublish:lib": "npm run package",
    "publish:lib": "npm publish ./dist/swimlane/ngx-cron",
    "prepack": "npm run package",
    "pack": "npm pack ./dist/swimlane/ngx-cron",
    "prettier": "prettier --write \"{src,projects}/**/*.{js,ts,scss,css,md,json}\"",
    "prettier:ci": "prettier --check  \"{src,projects}/**/*.{js,ts,scss,css,md,json}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@swimlane/ngx-ui": "^34.1.0",
    "codemirror": "^5.49.2",
    "cron-validate": "^1.4.0",
    "cronstrue": "^1.103.0",
    "moment-timezone": "^0.5.31",
    "ngx-moment": "^5.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular-devkit/build-ng-packagr": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@angular/language-service": "~10.1.0",
    "@swimlane/prettier-config-swimlane": "^3.0.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.1.2",
    "cross-env": "^7.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-config-swimlane": "^5.0.0",
    "typescript": "~4.0.2"
  }
}
